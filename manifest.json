{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Notes Downloader",
    "description": "Download notes from your course in Coursera.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "Click to download"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png"
    }
}